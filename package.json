{
  "name": "http-server",
  "version": "0.0.0",
  "description": "Native http server",
  "type": "module",
  "main": "./src/main.ts",
  "scripts": {
    "test": "node --test --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stdout --test-coverage-include='src/**/*.ts'",
    "test:ci": "node --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=coverage/lcov.info --test-coverage-include='src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'; tsc --project tsconfig.json",
    "build": "eslint 'src/**/*.ts' && tsc --build --verbose tsconfig.prod.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-security": "^3.0.0",
